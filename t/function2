use lib '.','..','../blib/lib','../blib/arch';
use CGI (':standard');
sub test {
    local($^W) = 0;
    my($num, $true,$msg) = @_;
    print($true ? "ok $num\n" : "not ok $num $msg\n");
}


test(23,param('weather') eq 'nice',"CGI::param() from POST");
test(24,url_param('big_balls') eq 'basketball',"CGI::url_param()");
